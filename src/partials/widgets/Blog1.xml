<b:widget id='Blog1' locked='true' title='Blog' type='Blog' visible='true' version='2'>
    <b:includable id='main' var='this'>
        <div class='flex w-full flex-col items-center justify-center'>
            <b:loop index='i' values='data:posts' var='post'>
                <article class='mb-8 flex w-full flex-row items-center justify-between last:mb-0 xl:mb-12'>
                    <div class='flex w-full flex-col items-start justify-center'>
                        <b:class expr:name='data:view.isSingleItem ? "no_class" : "ltr:mr-5 rtl:ml-5"' />
                        <b:comment>Index: Post Author</b:comment>
                        <b:if cond='data:allBylineItems.author'>
                            <div class='flex w-full flex-row items-center justify-start'>
                                <b:class expr:name='data:view.isSingleItem ? "mb-6 xl:mb-8" : "pb-2"' />
                                <div class='relative flex-shrink-0 flex-grow-0 overflow-hidden rounded-full'>
                                    <b:class expr:name='data:view.isSingleItem ? "h-12 w-12 ltr:mr-4 rtl:ml-4" : "h-5 w-5 ltr:mr-2 rtl:ml-2"' />
                                    <b:with value='data:post.author.authorPhoto.image ? data:post.author.authorPhoto.image : "//blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEhPzZlVciGeYVCgCEcfFKeNI8IhtFHNShFG_5Xavi8bej56zOpPRIjHEUZsTsLybpnEZkNJ6M-ivL0lcjLs9YEu_xVrD14a9gtcfxBAzIvwrmAY920GO9gkHtlljlwvRnLcRvD8WL5pbEcmgxCpct-7EJsMymUnpFCMxpTk8i0b0P7O82_kzKoEjeb3/s1600-rw/user-icon.webp"' var='images'>
                                        <span class='lazyload block h-full w-full flex-shrink-0 flex-grow-0 animate-pulse bg-[#cbd5e1] bg-cover bg-center bg-no-repeat' expr:title='data:post.author.name'>
                                            <b:attr name='data-image' expr:value='data:view.isSingleItem ? resizeImage(data:images, 48, "1:1") : resizeImage(data:images, 20, "1:1")' />
                                        </span>
                                    </b:with>
                                </div>
                                <b:tag cond='data:view.isPost' name='div' class='flex flex-col items-start justify-center'>
                                    <div class='flex flex-row flex-wrap items-center justify-center'>
                                        <b:class expr:name='data:view.isSingleItem ? "mb-1 text-base" : "text-[13px]"' />
                                        <div class='flex flex-row items-center justify-start ltr:pr-1 rtl:pl-1'>
                                            <b:class expr:name='data:view.isSingleItem ? "leading-6" : "max-h-4 font-medium leading-4"' />
                                            <b:if cond='data:allBylineItems.author.label'>
                                                <span class='hidden text-[#757575] ltr:mr-1 rtl:ml-1 md:block'>
                                                    <data:allBylineItems.author.label />
                                                </span>
                                            </b:if>
                                            <span class='notranslate text-[#292929]'>
                                                <data:post.author.name />
                                            </span>
                                        </div>
                                        <b:comment>Index: Post Label</b:comment>
                                        <b:if cond='data:blog.isMobileRequest and data:view.isMultipleItems and data:post.labels.notEmpty and data:allBylineItems.labels'>
                                            <div class='flex max-h-4 flex-row items-center justify-start font-medium leading-4 lg:hidden'>
                                                <b:with value='data:blog.locale == "id" ? "dalam" : "in"' var='languages'>
                                                    <span class='text-[#757575] ltr:mr-1 rtl:ml-1'>
                                                        <data:languages/>
                                                    </span>
                                                </b:with>
                                                <a expr:href='data:post.labels.first.url' expr:title='data:post.labels.first.name' rel='tag' class='text-[#292929]'>
                                                    <data:post.labels.first.name />
                                                </a>
                                            </div>
                                        </b:if>
                                    </div>
                                    <b:if cond='data:view.isPost'>
                                        <b:if cond='data:allBylineItems.timestamp and data:skin.vars.optionIndexTimestamp != "1px"'>
                                            <b:comment>Index: Post Timestamp</b:comment>
                                            <div class='flex flex-row items-center justify-center text-sm text-[#757575]'>
                                                <b:if cond='data:allBylineItems.timestamp.label'>
                                                    <span class='hidden ltr:mr-1 rtl:ml-1 md:block'>
                                                        <data:allBylineItems.timestamp.label />
                                                    </span>
                                                </b:if>
                                                <time expr:datetime='data:post.date.iso8601' class='flex flex-row items-center justify-center'>
                                                    <span expr:title='data:post.date'>
                                                        <data:post.date />
                                                    </span>
                                                    <b:if cond='!data:blog.isMobileRequest and data:post.date != data:post.lastUpdated'>
                                                        <span class='hidden before:px-[6px] before:content-["\2014"] xl:block' expr:title='data:post.lastUpdated'>
                                                            <data:post.lastUpdated />
                                                        </span>
                                                    </b:if>
                                                </time>
                                                <b:if cond='data:i in {0, 1, 2}'>
                                                    <svg width='15' height='15' viewBox='0 0 15 15' class='box-content h-[15px] w-[15px] px-[6px]'>
                                                        <path d='M7.44 2.32c.03-.1.09-.1.12 0l1.2 3.53a.29.29 0 0 0 .26.2h3.88c.11 0 .13.04.04.1L9.8 8.33a.27.27 0 0 0-.1.29l1.2 3.53c.03.1-.01.13-.1.07l-3.14-2.18a.3.3 0 0 0-.32 0L4.2 12.22c-.1.06-.14.03-.1-.07l1.2-3.53a.27.27 0 0 0-.1-.3L2.06 6.16c-.1-.06-.07-.12.03-.12h3.89a.29.29 0 0 0 .26-.19l1.2-3.52z' fill='currentColor' />
                                                    </svg>
                                                </b:if>
                                            </div>
                                        </b:if>
                                    </b:if>
                                </b:tag>
                            </div>
                        </b:if>
                        <b:comment>Post Title</b:comment>
                        <b:if cond='data:post.title != ""'>
                            <b:tag cond='data:post.link or (data:post.url and data:view.url != data:post.url)' name='a' expr:href='data:post.link ?: data:post.url' class='flex w-full flex-col items-center justify-center'>
                                <b:tag cond='data:view.isSingleItem' name='div' class='mb-10 flex w-full flex-col items-center justify-center xl:mb-14'>
                                    <b:tag expr:name='data:view.isMultipleItems ? "h2" : "h1"' expr:class='data:view.isSingleItem ? "w-full text-[32px] font-bold leading-10 text-[#292929]" : "w-full max-h-12 w-full text-ellipsis text-base font-bold leading-5 text-[#292929] line-clamp-2 md:max-h-[56px] md:text-[22px] md:leading-7"'>
                                        <data:post.title />
                                    </b:tag>
                                    <b:if cond='data:view.isMultipleItems'>
                                        <b:comment>Index: Post Summary</b:comment>
                                        <b:if cond='!data:blog.isMobileRequest'>
                                            <div class='hidden w-full pt-1 text-base leading-5 text-[#757575] md:text-ellipsis md:line-clamp-2'>
                                                <b:eval expr='data:post.snippets.short snippet {length: 120}' />
                                            </div>
                                        </b:if>
                                        <b:else />
                                        <b:comment>Item: Post Description</b:comment>
                                        <b:if cond='data:blog.metaDescription != ""'>
                                            <h2 class='w-full mt-3 text-[22px] font-normal leading-7 text-[#757575]'>
                                                <data:blog.metaDescription />
                                            </h2>
                                        </b:if>
                                    </b:if>
                                </b:tag>
                            </b:tag>
                        </b:if>
                        <b:if cond='data:view.isMultipleItems'>
                            <div class='flex w-full flex-row items-center justify-between pt-2'>
                                <div class='flex flex-row items-center justify-center text-[13px] text-[#757575]'>
                                    <b:if cond='data:allBylineItems.timestamp and data:skin.vars.optionIndexTimestamp != "1px"'>
                                        <b:comment>Index: Post Timestamp</b:comment>
                                        <div class='flex flex-row items-center justify-center'>
                                            <b:class cond='!data:blog.isMobileRequest and data:post.labels.notEmpty and data:allBylineItems.labels' name='after:hidden after:px-[6px] after:content-["\00B7"] xl:after:block' />
                                            <b:if cond='data:allBylineItems.timestamp.label'>
                                                <span class='hidden ltr:mr-1 rtl:ml-1 md:block'>
                                                    <data:allBylineItems.timestamp.label />
                                                </span>
                                            </b:if>
                                            <time expr:datetime='data:post.date.iso8601' class='flex flex-row items-center justify-center'>
                                                <span expr:title='data:post.date'>
                                                    <data:post.date />
                                                </span>
                                                <b:if cond='!data:blog.isMobileRequest and data:post.date != data:post.lastUpdated'>
                                                    <span class='hidden before:px-[6px] before:content-["\2014"] xl:block' expr:title='data:post.lastUpdated'>
                                                        <data:post.lastUpdated />
                                                    </span>
                                                </b:if>
                                            </time>
                                        </div>
                                    </b:if>
                                    <b:comment>Index: Post Label</b:comment>
                                    <b:if cond='!data:blog.isMobileRequest and data:post.labels.notEmpty and data:allBylineItems.labels'>
                                        <a expr:href='data:post.labels.first.url' expr:title='data:post.labels.first.name' rel='tag' class='relative hidden whitespace-nowrap rounded-full bg-[#f2f2f2] py-[2px] px-2 leading-5 transition-colors hover:bg-[#e6e6e6] lg:block'>
                                            <data:post.labels.first.name />
                                        </a>
                                    </b:if>
                                    <b:if cond='data:i in {0, 1, 2}'>
                                        <svg width='15' height='15' viewBox='0 0 15 15' class='box-content h-[15px] w-[15px] px-[6px]'>
                                            <path d='M7.44 2.32c.03-.1.09-.1.12 0l1.2 3.53a.29.29 0 0 0 .26.2h3.88c.11 0 .13.04.04.1L9.8 8.33a.27.27 0 0 0-.1.29l1.2 3.53c.03.1-.01.13-.1.07l-3.14-2.18a.3.3 0 0 0-.32 0L4.2 12.22c-.1.06-.14.03-.1-.07l1.2-3.53a.27.27 0 0 0-.1-.3L2.06 6.16c-.1-.06-.07-.12.03-.12h3.89a.29.29 0 0 0 .26-.19l1.2-3.52z' fill='currentColor' />
                                        </svg>
                                    </b:if>
                                </div>
                                <svg width="25" height="25" viewBox="0 0 25 25" fill="none" class="text-[#292929]">
                                    <path d="M18 2.5a.5.5 0 0 1 1 0V5h2.5a.5.5 0 0 1 0 1H19v2.5a.5.5 0 1 1-1 0V6h-2.5a.5.5 0 0 1 0-1H18V2.5zM7 7a1 1 0 0 1 1-1h3.5a.5.5 0 0 0 0-1H8a2 2 0 0 0-2 2v14a.5.5 0 0 0 .8.4l5.7-4.4 5.7 4.4a.5.5 0 0 0 .8-.4v-8.5a.5.5 0 0 0-1 0v7.48l-5.2-4a.5.5 0 0 0-.6 0l-5.2 4V7z" fill="currentColor"></path>
                                </svg>
                            </div>
                            <b:else />
                            <b:comment>Item: Post Content</b:comment>
                            <div id='post_body' class='post_body prose prose-headings:font-webium lg:leading-8'>
                                <data:post.body />
                            </div>
                        </b:if>
                    </div>
                    <b:if cond='data:view.isMultipleItems'>
                        <b:comment>Index: Post Thumbnail</b:comment>
                        <a class='flex h-[100px] w-[100px] flex-shrink-0 flex-grow-0 items-end justify-center bg-slate-300 md:h-[134px] md:w-[200px]' expr:href='data:post.url' expr:title='data:post.title'>
                            <b:class cond='data:post.featuredImage.isYouTube' name='youtube_image' />
                            <b:with value='data:post.featuredImage.isYouTube ? data:post.featuredImage.youtubeMaxResDefaultUrl : data:post.featuredImage ?: "//lh3.googleusercontent.com/-Efigs16DUTQ/YKrJkJaHDKI/AAAAAAAAg_A/_xTHzW1sLmssSTAFEZtZTL_QqseedXjfACLcBGAsYHQ/s1600-rw/index.webp"' var='images'>
                                <span class='lazyload block h-full w-full flex-shrink-0 flex-grow-0 animate-pulse bg-[#cbd5e1] bg-cover bg-center bg-no-repeat' expr:title='data:post.title' expr:data-image='resizeImage(data:images, 200, "100:67")'></span>
                            </b:with>
                        </a>
                    </b:if>
                </article>
            </b:loop>
        </div>
    </b:includable>

    <b:includable id='aboutPostAuthor' />
    <b:includable id='addComments' />
    <b:includable id='blogThisShare' />
    <b:includable id='bylineByName' var='byline' />
    <b:includable id='bylineRegion' var='regionItems' />
    <b:includable id='commentAuthorAvatar' />
    <b:includable id='commentDeleteIcon' var='comment' />
    <b:includable id='commentForm' var='post' />
    <b:includable id='commentFormIframeSrc' var='post' />
    <b:includable id='commentItem' var='comment' />
    <b:includable id='commentList' var='comments' />
    <b:includable id='commentPicker' var='post' />
    <b:includable id='comments' var='post' />
    <b:includable id='commentsLink' />
    <b:includable id='commentsLinkIframe' />
    <b:includable id='commentsTitle' />
    <b:includable id='defaultAdUnit' />
    <b:includable id='emailPostIcon' />
    <b:includable id='facebookShare' />
    <b:includable id='feedLinks' />
    <b:includable id='feedLinksBody' var='links' />
    <b:includable id='footerBylines' />
    <b:includable id='googlePlusShare' />
    <b:includable id='headerByline' />
    <b:includable id='homePageLink' />
    <b:includable id='iframeComments' var='post' />
    <b:includable id='linkShare' var='post' />
    <b:includable id='nextPageLink' />
    <b:includable id='otherSharingButton' />
    <b:includable id='platformShare' />
    <b:includable id='post' var='post' />
    <b:includable id='postAuthor' />
    <b:includable id='postBody' var='post' />
    <b:includable id='postBodySnippet' var='post' />
    <b:includable id='postCommentsAndAd' var='post' />
    <b:includable id='postCommentsLink' />
    <b:includable id='postFooter' var='post' />
    <b:includable id='postFooterAuthorProfile' var='post' />
    <b:includable id='postHeader' var='post' />
    <b:includable id='postJumpLink' var='post' />
    <b:includable id='postLabels' />
    <b:includable id='postLocation' />
    <b:includable id='postMeta' var='post' />
    <b:includable id='postMetadataJSONImage' />
    <b:includable id='postMetadataJSONPublisher' />
    <b:includable id='postPagination' />
    <b:includable id='postReactions' />
    <b:includable id='postShareButtons' />
    <b:includable id='postTimestamp' />
    <b:includable id='postTitle' var='post' />
    <b:includable id='previousPageLink' />
    <b:includable id='sharingButton' />
    <b:includable id='sharingButtonContent' />
    <b:includable id='sharingButtons' />
    <b:includable id='sharingButtonsMenu' />
    <b:includable id='sharingPlatformIcon' />
    <b:includable id='threadedCommentForm' var='post' />
    <b:includable id='threadedCommentJs' var='post' />
    <b:includable id='threadedComments' var='post' />
</b:widget>