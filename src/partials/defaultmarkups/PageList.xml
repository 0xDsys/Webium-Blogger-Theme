<b:includable id='main'>
    <b:class name='mb-6 w-full border-b border-[#e6e6e6] pb-6 last:mb-0 last:border-b-0 last:pb-0' />
    <b:include name='widget-title' />
    <div class='relative flex flex-row flex-wrap items-center justify-start overflow-hidden text-sm'>
        <b:loop values='data:links' var='link'>
            <a class='mb-2 text-[#757575] ltr:mr-[26px] rtl:ml-[26px]' expr:href='data:link.href' expr:title='data:link.title'>
                <b:class cond='data:link.isCurrentPage' name='text-[#292929]' />
                <data:link.title />
            </a>
        </b:loop>
    </div>
</b:includable>
<b:includable id='content' />
<b:includable id='pageLink' />
<b:includable id='pageList' />
<b:includable id='overflowButton' />
<b:includable id='overflowablePageList' />
