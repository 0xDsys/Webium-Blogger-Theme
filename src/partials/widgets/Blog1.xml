                                            <span class='lazyload block h-full w-full flex-shrink-0 flex-grow-0 animate-pulse bg-[#cbd5e1] bg-cover bg-center bg-no-repeat' expr:title='data:post.author.name'>
                                                <b:attr name='data-image' expr:value='data:view.isSingleItem ? resizeImage(data:images, 48, "1:1") : resizeImage(data:images, 20, "1:1")' />
                                            </span>
                                        </b:with>
                                    </div>
                                    <b:tag cond='data:view.isPost' name='div' class='flex flex-col items-start justify-center'>
                                        <div class='flex flex-row flex-wrap items-center justify-center'>
                                            <b:class expr:name='data:view.isSingleItem ? "mb-1 text-base" : "text-[13px]"' />
                                            <div class='flex flex-row items-center justify-start ltr:pr-1 rtl:pl-1'>
                                                <b:class expr:name='data:view.isSingleItem ? "leading-6" : "max-h-4 font-medium leading-4"' />
                                                <b:if cond='data:allBylineItems.author.label'>
                                                    <span class='hidden text-[#757575] ltr:mr-1 rtl:ml-1 md:block'>
                                                        <data:allBylineItems.author.label />
                                                    </span>
                                                </b:if>
                                                <span class='notranslate text-[#292929]'>
                                                    <data:post.author.name />
                                                </span>
                                            </div>
                                            <b:comment>Index: Post Label</b:comment>
